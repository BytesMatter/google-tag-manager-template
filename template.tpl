___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setInWindow = require('setInWindow');
const queryPermission = require('queryPermission');
const injectScript = require('injectScript');


// Template value for website key
const account = data.key;
const beaconUrl = 'https://www.bytesmatter.io/dashboard/beacon_gtm.min.js';

// Setup the config object
setInWindow('_perfWeb_', {
    key: account,
    errors: [],
    scriptManager: 'googleTagManager'
}, true);

// Inject the main script
if (queryPermission('inject_script', beaconUrl)) {
  injectScript(beaconUrl);
   data.gtmOnSuccess();
} else {
  data.gtmOnFailure();
}


